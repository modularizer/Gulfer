{
  "name": "gulfer",
  "version": "0.1.0",
  "description": "A cross-platform golf and disc golf tracking application",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --clear",
    "build:web": "expo export -p web --output-dir dist && cp assets/favicon.ico dist/favicon.ico",
    "create-app-icon": "node scripts/create-app-icon.js",
    "generate-android-icons": "node scripts/generate-android-icons.js",
    "db:generate": "npx tsx storage/schema/generate-all-create-scripts.ts",
    "db:test": "npx tsx storage/tests/golf/test-web.ts",
    "db:test:web": "expo start --web",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@sqlite.org/sqlite-wasm": "^3.51.1-build1",
    "drizzle-orm": "^0.44.7",
    "expo": "~51.0.0",
    "expo-camera": "~15.0.4",
    "expo-constants": "~16.0.2",
    "expo-crypto": "^15.0.7",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image": "~1.13.0",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.24",
    "expo-sqlite": "^16.0.9",
    "expo-status-bar": "~1.12.1",
    "localforage": "^1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.13",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "better-sqlite3": "^11.0.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^8.57.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "sharp": "^0.34.5",
    "ts-morph": "^27.0.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
