{
  "expo": {
    "name": "Gulfer",
    "slug": "gulfer",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "gulfer",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gulfer.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gulfer.app",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "Gulfer",
      "shortName": "Gulfer",
      "lang": "en",
      "scope": "/",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "manifest": "./public/manifest.json",
      "serviceWorker": "./public/sw.js"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Gulfer to access your photos to attach them to rounds."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Gulfer to access your camera to take photos for rounds."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "baseUrl": "/Gulfer"
    }
  }
}
